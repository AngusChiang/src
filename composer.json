{
    "name": "fastsupport/api",
    "type": "project",
    "description": "闪客服API代码.",
    "keywords": [
        "laravel",
        "kefu",
        "customer-service",
        "support"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "023pay/sdk": "^0.0.1",
        "023sec/svg-captcha": "dev-master",
        "doctrine/dbal": "^2.11",
        "fastsupport-cn/province-city-area": "1.2.2",
        "fideloper/proxy": "^4.2",
        "filp/whoops": "^2.7",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "iidestiny/laravel-filesystem-oss": "^2.0",
        "itbdw/ip-database": "^2.0",
        "laravel-notification-channels/webpush": "^5.0",
        "laravel/framework": "^8.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "minishlink/web-push": "^6.0",
        "piwik/device-detector": "^3.13",
        "predis/predis": "^1.1",
        "spatie/laravel-permission": "^3.17",
        "squizlabs/php_codesniffer": "^3.5",
        "tymon/jwt-auth": "^1.0",
        "vinkla/hashids": "^9.0",
        "xiaohuilam/laravel-aliyun-waf-trustedproxy": "^0.0.1",
        "yansongda/laravel-pay": "^2.3"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.8",
        "beyondcode/laravel-dump-server": "^1.4",
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan jwt:secret"
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php artisan jwt:secret"
        ]
    }
}
